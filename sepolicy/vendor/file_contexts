
# TTY devices
/dev/ttyUSB0                                                                    u:object_r:tty_device:s0
/dev/ttyACM0                                                                    u:object_r:tty_device:s0

# Video devices
/dev/media0                                                                     u:object_r:video_device:s0
/dev/v4l-subdev0                                                                u:object_r:video_device:s0
/dev/v4l-subdev1                                                                u:object_r:video_device:s0
/dev/v4l-subdev2                                                                u:object_r:video_device:s0

# Hihope HALs
/vendor/bin/hw/android.hardware.sensors@1.0-service.smarc_rzg2l                 u:object_r:hal_sensors_default_exec:s0

# GPU device
/dev/mali0                                                                      u:object_r:gpu_device:s0

# Zram swap
/dev/block/zram0                                                                u:object_r:swap_block_device:s0
